    <Resource Include="Resources\sprinkler.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Resources\temperature.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
