<Window x:Class="Automate.Views.GreenHouseWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Automate.Views"
        mc:Ignorable="d"
        Title="GreenHouseWindow" Height="450" Width="800">
    <Grid Margin="10,10,10,10">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*" />
            <ColumnDefinition Width="1*" />
            <ColumnDefinition Width="1*" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*" />
            <RowDefinition Height="1*" />
            <RowDefinition Height="1*" />
        </Grid.RowDefinitions>
        <Image Grid.Column="0" Grid.Row="0" Source="/Resources/fan.png" Width="64" Height="64" />
        <Image Grid.Column="1" Grid.Row="0" Source="/Resources/heater.png" Width="64" Height="64" />
        <Image Grid.Column="0" Grid.Row="1" Source="/Resources/bulb.png" Width="64" Height="64" />
        <Image Grid.Column="1" Grid.Row="1" Source="/Resources/sprinkler.png" Width="64" Height="64" />
        <Image Grid.Column="0" Grid.Row="2" Source="/Resources/window.png" Width="64" Height="64" />
    </Grid>
</Window>
